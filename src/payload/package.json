{
  "name": "zervios-payload",
  "version": "1.0.0",
  "description": "Payload CMS for ZerviOS",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js payload build",
    "build:server": "tsc",
    "build": "tsc && cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js payload build",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js payload generate:graphQLSchema"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-mongodb": "^1.7.5",
    "@payloadcms/richtext-slate": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "payload": "^2.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
