  "dependencies": {
    "payload": "^1.6.0",
    "express": "^4.18.2",
    "mongodb": "^5.2.0"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "nodemon": "^2.0.22",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1"
  }
}